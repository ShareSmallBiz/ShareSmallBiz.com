{
  "ShareSmallBizUserContext": "Data Source=c:\\websites\\ShareSmallBiz\\ShareSmallBizUser.db",
  "AllowedHosts": "*",
  "DefaultGetPath": "",
  "JwtSettings": {
    "Secret": "YourSuperSecretKeyThatShouldBeAtLeast32CharactersLong!",
    "Issuer": "ShareSmallBiz.Portal",
    "Audience": "ShareSmallBiz.Portal.Users",
    "ExpirationInHours": 1
  },
  "HttpRequestResultPollyOptions": {
    "MaxRetryAttempts": 3,
    "RetryDelaySeconds": 1,
    "CircuitBreakerThreshold": 3,
    "CircuitBreakerDurationSeconds": 10
  },
  "ShareSmallBizMedia": {
    "UploadPath": "C:\\websites\\ShareSmallBiz\\Media\\"
  },
  "SerilogLevels": "Debug, Information, Warning, Error, Fatal",
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\websites\\ShareSmallBiz\\logs\\ShareSmallBiz-.txt",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "ShareSmallBiz": {
    "LogFilePath": "C:\\websites\\ShareSmallBiz\\logs\\ShareSmallBiz-.txt"
  }
}